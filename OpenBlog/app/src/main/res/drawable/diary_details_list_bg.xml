<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 第一层  上部和左部偏移一定距离-->

    <item
        android:left="2.66dp"
        android:top="2.66dp">
        <shape>
            <solid android:color="#f5f5f5" />
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="#efebeb" />
        </shape>
    </item>
    <!-- 第二层 下部和上部偏移一定距离-->
    <item
        android:bottom="2.66dp"
        android:right="2.66dp">
        <shape>
            <solid android:color="#ffffff" />
            <!-- 描边 -->
            <stroke android:width="0.3dp" android:color="#dedede" />
        </shape>
    </item>

</layer-list>